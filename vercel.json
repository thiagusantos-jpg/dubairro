{
  "buildCommand": "echo 'Build skipped for static site'",
  "outputDirectory": ".",
  "public": true,
  "functions": {
    "api/data.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/data/:file",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/api/:path",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/data/(.*)\\.json$",
      "dest": "/data/$1.json",
      "methods": ["GET"]
    },
    {
      "src": "/(?!api/)(?!data/).*",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
